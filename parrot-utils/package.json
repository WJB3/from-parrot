{
  "name": "parrot-utils",
  "version": "1.0.0",
  "description": "some utils of JS",
  "main": "index.js",
  "scripts": {
    "test": "echo npm_package_name",
    "tscinit": "tsc --init",
    "tscmd": "tsc merge-descriptor/core/index --outDir merge-descriptor/src --target esnext",
    "tscwt": "tsc which-type/core/index --outDir which-type/src --target esnext",
    "tschmc": "tsc http-methods/core/index --outDir http-methods/src --target esnext",
    "tschmb": "tsc http-methods/core/browser --outDir http-methods/src --target esnext",
    "tschf": "tsc has-flag/core/index --outDir has-flag/src --target esnext",
    "tscscc": "tsc supports-color/core/index --outDir supports-color/src --target esnext",
    "tscscb": "tsc supports-color/core/browser --outDir supports-color/src --target esnext",
    "tscnoop": "tsc noop/core/index --outDir noop/src --target esnext",
    "tscms": "tsc ms/core/index --outDir ms/src --target esnext",
    "tscdebugb": "tsc debug/core/browser --outDir debug/src --target esnext",
    "babelmd": "babel merge-descriptor/src/index.js -d merge-descriptor/tran",
    "babelwt": "babel which-type/src/index.js -d which-type/tran",
    "babelhmc": "babel http-methods/src/index.js -d http-methods/tran",
    "babelhmb": "babel http-methods/src/browser.js -d http-methods/tran",
    "babelhf": "babel has-flag/src/index.js -d has-flag/tran",
    "babelscc": "babel supports-color/src/index.js -d supports-color/tran",
    "babelscb": "babel supports-color/src/browser.js -d supports-color/tran",
    "babelnoop": "babel noop/src/index.js -d noop/tran",
    "babelms": "babel ms/src/index.js -d ms/tran",
    "babeldebugb": "babel debug/src/browser.js debug/src/common.js -d debug/tran",
    "rollupmd": "rollup merge-descriptor/tran/index.js --file merge-descriptor/lib/index.js --format umd --name 'mergeDescriptor'",
    "rollupwt": "rollup which-type/tran/index.js --file which-type/lib/index.js --format umd --name 'whichType'",
    "rolluphmc": "rollup http-methods/tran/index.js --file http-methods/lib/index.js --format umd --name 'HTTPMETHODS'",
    "rolluphmb": "rollup http-methods/tran/browser.js --file http-methods/lib/browser.js  --format umd --name 'HTTPMETHODS'",
    "rolluphf": "rollup has-flag/tran/index.js --file has-flag/lib/index.js --format cjs",
    "rollupscc": "rollup supports-color/tran/index.js --file supports-color/lib/index.js --format cjs",
    "rollupscb": "rollup supports-color/tran/browser.js --file supports-color/lib/browser.js --format umd --name 'supportsColor'",
    "rollupnoop": "rollup noop/tran/index.js --file noop/lib/index.js --format umd --name 'NOOP'",
    "rollupms": "rollup ms/tran/index.js --file ms/lib/index.js --format umd --name 'MS'",
    "rollupdebugb": "rollup debug/tran/browser.js --file debug/lib/browser.js --format umd --name 'DEBUG'",
    "testmd": "node merge-descriptor/test/index.js",
    "testwt": "node which-type/test/index.js",
    "testhm": "node http-methods/test/index.js",
    "testhf": "node has-flag/test/index.js -f --unicorn --foo=bar -- --rainbow",
    "testscc": "node supports-color/test/index.js",
    "testnoop": "node noop/test/index.js",
    "testms": "node ms/test/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/parrot-design/parrot-utils.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/parrot-design/parrot-utils/issues"
  },
  "homepage": "https://github.com/parrot-design/parrot-utils#readme",
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/runtime": "^7.13.10",
    "@parrotjs/http-methods": "^1.2.0",
    "@parrotjs/which-type": "^1.5.0",
    "@types/node": "^14.14.34",
    "debug": "^4.3.1",
    "rollup": "^2.41.2",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@parrotjs/supports-color": "^2.1.0",
    "recorder-core": "^1.1.21021500"
  }
}
